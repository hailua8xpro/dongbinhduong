@model IEnumerable<NamTanUyen.Models.PropertyDetail>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<p>
    @Html.ActionLink("Create New", "Create", new { propertyid = ViewBag.propertyId })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Property.PropertyId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PropertyValue)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Property.PropertyId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PropertyValue)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.PropertyDetailId }) |
                @Html.ActionLink("Details", "Details", new { id = item.PropertyDetailId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.PropertyDetailId })
            </td>
        </tr>
    }

</table>

