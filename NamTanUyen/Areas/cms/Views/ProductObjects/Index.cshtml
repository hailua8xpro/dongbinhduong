@model IEnumerable<NamTanUyen.Models.ProductObject>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.ProductObjectName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ImageUrl)
        </th>
        <th>Thao tác</th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ProductObjectName)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
       
        <td>
            <img src="@item.ImageUrl" width="100" alt="" />
        </td>
      
        <td>
            @Html.ActionLink("Chỉnh sửa", "Edit", new { id=item.ProductObjectId }) |
            @Html.ActionLink("Xóa", "Delete", new { id=item.ProductObjectId })
        </td>
    </tr>
}

</table>
